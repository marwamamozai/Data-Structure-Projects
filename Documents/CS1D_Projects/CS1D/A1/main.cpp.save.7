  //BUBBLE SORT                                                                                                                                                       
                                                                                                                                                                                                          
  void bubbleSort ( int A [], int n ) {                                                                                                                                                                   
                                                                                                                                                                                                          
  for ( int i = 0; i < n -1 ; ++i) {                                                                                                                                                                     
                                                                                                                                                                                                         
    for ( int j = n - 1; j > i ; --j{                                                                                                                                                                    
                                                                                                                                                                                                         
	 if ( A[j] < A[j-1]) {                                                                                                                                                                              
        // swap A[j} and A[j-1]                                                                                                                                                                          
         int temp = A[j];                                                                                                                                                                                
                                                                                                                                                                                                         
         A[j] = A[j-1];                                                                                                                                                                                  
          A[j-1] = temp;                                                                                                                                                                                  
       } // end if                                                                                                                                                                                       
     } // end (for int j)                                                                                                                                                                                
    } // end (for int i)                                                                                                                                                                                  
 } //end function                                                                                                                                                                                        
                                                                                                                                                                                                         
                                                                                                                                                                                                         
                                                                                                                                                                                                         
