 //INSERTION SORT                                                                                                                                                                                        
                                                                                                                                                                                                         
 void insertionSort (int A[], int n ) {                                                                                                                                                                  
                                                                                                                                                                                                         
  for ( int j = 2; j<=n; ++j){                                                                                                                                                                           
   int i = j - 1;                                                                                                                                                                                        
   int key = A[j];                                                                                                                                                                                       
                                                                                                                                                                                                         
    while ( i > 0 && A[i] > key) {                                                                                                                                                                        
     A[i+1] = A[i];                                                                                                                                                                                      
     --i;                                                                                                                                                                                                
    }// end while                                                                                                                                                                                        
                                                                                                                                                                                                         
    A[i+1] = key;                                                                                                                                                                                        
  }// end for                                                                                                                                                                                           
 }//end function                                                                                                                                                                                                                                                                                               
